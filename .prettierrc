printWidth: 80
singleQuote: true
semi: true
bracketSpacing: true
parser: babylon
trailingComma: 'all'
overrides:
- files:
  - '*.ts'
  - '*.tsx'
  options:
    parser: typescript
- files: '*.md'
  options:
    parser: markdown
- files: '*.yml'
  options:
    parser: yaml
- files: '*.json'
  options:
    parser: json
    printWidth: 80
- files: '*.css'
  options:
    parser: css
